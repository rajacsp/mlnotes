   "source": [
    "fn = \"DC_QnA_bo_v.15.12.3_DE_duplicates.xlsx\"\n",
    "rgx = re.compile('\\b_[A-Z]{2}\\b')\n",
    "print(re.findall(rgx, fn))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['DE']\n"
     ]
    }
   ],
   "source": [
    "rgx = re.compile('(?<=_)[A-Z]+(?=_)')\n",
    "print(re.findall(rgx, fn))"
   ]
