   "source": [
    "from datetime import datetime"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "cf260179-e8a8-4f19-87cd-f21a9c6d43a2",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_age(d):\n",
    "    d1 = datetime.now()\n",
    "    months = (d1.year - d.year) * 12 + d1.month - d.month\n",
    "\n",
    "    year = int(months / 12)\n",
    "    return year"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "db2aca8b-2573-4ba4-8521-63fe8ff1ffc4",
   "metadata": {},
   "outputs": [],
   "source": [
    "age = get_age(datetime(1991, 1, 1))"
   ]
  },
