    "y = np.random.rand(10)\n",
    "z = np.sqrt(x**2 + y**2)\n",
    "\n",
    "plt.subplot(111)\n",
    "plt.scatter(x, y, s=500, c=z, marker=\">\")\n",
    "\n",
    "plt.show()"
